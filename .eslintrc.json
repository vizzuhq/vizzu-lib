{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": 1,
        "no-tabs": 2,
        "no-cond-assign": 2,
        "no-console": 1,
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 1,
        "no-dupe-keys": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 1,
        "no-extra-semi": 1,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 1,
        "valid-typeof": 2,

        "consistent-return": 1,
        "curly": [1, "all"],
        "dot-notation": 1,
        "eqeqeq": [2, "smart"],
        "guard-for-in": 1,
        "no-alert": 1,
        "no-caller": 1,
        "no-div-regex": 1,
        "no-else-return": 1,
        "no-eq-null": 1,
        "no-eval": 2,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-fallthrough": 1,
        "no-floating-decimal": 0,
        "no-implied-eval": 2,
        "no-iterator": 1,
        "no-labels": 1,
        "no-lone-blocks": 2,
        "no-loop-func": 1,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 1,
        "no-octal-escape": 1,
        "no-proto": 1,
        "no-redeclare": 1,
        "no-return-assign": 1,
        "no-script-url": 1,
        "no-self-compare": 2,
        "no-sequences": 1,
        "no-unused-expressions": 0,
        "no-void": 1,
        "no-warning-comments": 0,
        "no-with": 2,
        "radix": 2,
        "vars-on-top": 0,
        "wrap-iife": [2, "any"],
        "yoda": 0,

        "global-strict": 0,
        "strict": 1,

        "no-catch-shadow": 1,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow": 1,
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-undefined": 1,
        "no-undef-init": 1,
        "no-unused-vars": 1,
        "no-use-before-define": 1,

        "brace-style": 0,
        "camelcase": 0,
        "comma-spacing": 0,
        "comma-style": [1, "last"],
        "consistent-this": 0,
        "eol-last": 1,
        "func-names": 0,
        "func-style": 0,
        "key-spacing": 0,
        "max-nested-callbacks": 0,
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-nested-ternary": 0,
        "no-new-object": 2,
        "semi-spacing": [1, { "before": false, "after": true }],
        "no-spaced-func": 1,
        "no-ternary": 0,
        "no-trailing-spaces": 1,
        "no-multiple-empty-lines": 0,
        "no-underscore-dangle": 0,
        "one-var": 0,
        "padded-blocks": 0,
        "quotes": 0,
        "quote-props": [1, "as-needed"],
        "semi": [2, "always"],
        "sort-vars": 0,
        "space-after-keywords": 0,
        "space-before-blocks": 0,
        "space-in-brackets": 0,
        "space-in-parens": 0,
        "space-infix-ops": 0,
        "space-unary-ops": 1,
        "spaced-line-comment": 0,
        "wrap-regex": 0
    }
}
